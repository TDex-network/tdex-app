    "@capacitor/core": "^2.4.7",
    "@capacitor/ios": "^2.4.7",
    "@cheprasov/react-qrcode": "^1.0.1",
    "@ionic-native/clipboard": "^5.33.0",
    "@ionic-native/core": "^5.33.0",
    "@ionic-native/screen-orientation": "^5.33.0",
    "@ionic/react": "^5.6.7",
    "@ionic/react-router": "^5.6.7",
    "axios": "^0.21.1",
    "buffer-json": "^2.0.0",
    "capacitor-secure-storage-plugin": "^0.5.0",
    "classnames": "^2.3.1",
    "cordova-clipboard": "^1.3.0",
    "es6-promise-plugin": "^4.2.2",
    "ionicons": "^5.5.1",
    "jetifier": "^1.6.8",
    "liquidjs-lib": "^5.1.12",
    "moment": "^2.29.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "scrypt-js": "^3.0.1",
    "tdex-sdk": "1.6.7"
  },
  "scripts": {
    "serve": "npx ionic serve",
    "android": "npx ionic capacitor run android",
    "android:serve": "npx ionic capacitor run android -l --external",
    "ios": "npx ionic capacitor run ios",
    "ios:serve": "ionic capacitor run ios -l --external",
    "build:android": "npx ionic capacitor build android --no-open",
    "build:ios": "npx ionic capacitor build ios --no-open",
    "sync": "npx jetify && npx ionic cap sync",
    "lint": "npx eslint './src/**/*.ts*'",
    "lint:fix": "npx eslint './src/**/*.ts*' --fix",
    "test": "BABEL_ENV=test npx jest",
    "build": "npx ionic build",
    "resources": "npx cordova-res ios --skip-config --copy && npx cordova-res android --skip-config --copy",
    "tdex:run": "bash scripts/tdexd-run.sh",
    "tdex:clean": "bash scripts/tdexd-clean.sh"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@capacitor/cli": "^2.4.7",
    "@ionic/cli": "^6.16.1",
    "@ionic/eslint-plugin": "0.0.1",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@types/buffer-json": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.3.0",
    "@types/react": "^16.14.6",
    "@types/react-dom": "^16.9.13",
    "@types/react-redux": "^7.1.16",
    "@types/react-router": "^5.1.14",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "cordova-res": "^0.15.3",
    "eslint-config-prettier": "^8.3.0",
    "jest-canvas-mock": "^2.3.1",
    "node-sass": "^4.14.1",
    "prettier": "^2.3.0",
    "redux-devtools-extension": "^2.13.9",
    "tdex-react-scripts": "4.0.3-tdex",
    "ts-jest": "^26.5.6",
    "typescript": "3.8.3"
  },
  "description": "Mobile app for making trades on TDEX",
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": false
  }
}
